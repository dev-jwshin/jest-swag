{
  "name": "@foryourdev/jest-swag",
  "version": "1.0.18",
  "description": "Generate OpenAPI/Swagger documentation from your Jest API tests",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jest-swag": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --reporters=default --reporters=./dist/reporter.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --reporters=default --reporters=./dist/reporter.js",
    "test:docs": "jest --reporters=./dist/reporter.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "generate:docs": "node dist/cli.js"
  },
  "keywords": [
    "jest",
    "api",
    "testing",
    "openapi",
    "swagger",
    "documentation",
    "typescript",
    "express",
    "nestjs",
    "api-testing",
    "test-driven-development",
    "swagger-ui"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/dev-jwshin/jest-swag#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-jwshin/jest-swag.git"
  },
  "bugs": {
    "url": "https://github.com/dev-jwshin/jest-swag/issues"
  },
  "dependencies": {
    "swagger-ui-dist": "^5.10.5",
    "express": "^4.18.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "@jest/test-result": "^29.7.0",
    "@jest/types": "^29.6.3"
  },
  "peerDependencies": {
    "jest": ">=27.0.0",
    "express": ">=4.0.0",
    "@nestjs/common": ">=8.0.0",
    "@nestjs/core": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md"
  ]
}
